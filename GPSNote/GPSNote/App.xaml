        <ImageSource x:Key="pic_enter_page_big">pic_enter_page2x.png</ImageSource>
        <ImageSource x:Key="ic_google">ic_google.png</ImageSource>
        <ImageSource x:Key="ic_left_blue">ic_left_blue.png</ImageSource>
