        <ImageSource x:Key="ic_like_blue">ic_like_blue.png</ImageSource>
        <ImageSource x:Key="ic_like_gray">ic_like_gray.png</ImageSource>
        <ImageSource x:Key="ic_eye">ic_eye.png</ImageSource>
